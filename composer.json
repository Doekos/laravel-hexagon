{
    "name": "doekos/laravel-hexagon",
    "description": "Package to help you setup DDD in your Laravel Project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Doekos\\LaravelHexagon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Doekos\\LaravelHexagon\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "authors": [
        {
            "name": "Bas Visser"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.37"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Doekos\\LaravelHexagon\\LaravelHexagonServiceProvider"
            ]
        }
    }
}
